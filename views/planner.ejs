<!-- DIV WITH MAP AND FIELD TO SEARCH & CONFIRM TRIP DESTINATION -->

<input id="pac-input" class="controls" type="text" placeholder="Enter destinatinon">

<div id="search-div">
	<div id="map"></div>
	<input class="confirm-dest-button" type="button" value="Confirm destination">
</div>


<!-- DIV IN WHICH THE SUGGESTIONS FROM GOOGLE PLACES WILL BE SHOWN -->

<div id="edit-trip-div">
	<h1 class="trip-destination"></h1>
	
	<div id="edit-trip-content">
	</div>

	<input type="button" id="done-button" value="DONE!" />
</div>


<!-- DIV IN WHICH THE TRIP SUMMARY WILL BE SHOWN -->
<div id="show-trip-div">
	<h1 class="trip-destination"></h1>

	<div id="show-trip-content">	
	</div>

	<input type="button" id="edit-trip-button" value="Edit trip" />

</div>


<!-- DIV IN WHICH THE TRIPS LIST FOR THE USER WILL BE SHOWN -->
<div id="trips-list">
</div>





